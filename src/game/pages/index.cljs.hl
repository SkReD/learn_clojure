(page "index.html"
      (:require [game.logic] [debug.repl]))

(def currentMarkType (cell :crosses))
(def board @game.logic/board)

(html
  (body
    (div :class "game-table"
      (loop-tpl :bindings [row board]
        (div :class "game-row"
             (loop-tpl :bindings [row-cell row]
                       (span :class "game-cell" :click #(js/console.log 4) row-cell)))))))

